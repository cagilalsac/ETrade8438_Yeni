@*@model List<Business.Models.ProductModel>*@
@model IEnumerable<ProductModel>

@{
    ViewData["Title"] = "Products";
}

@*<h3>@ViewData["Title"]</h3>*@
<h3>@ViewBag.Title</h3>

<table class="table">
    <tr>
        <th>
            Name
        </th>
    </tr>
    @foreach (var product in Model)
    {
        <tr>
            <td>
                @product.Name
            </td>
        </tr>
    }
</table>

